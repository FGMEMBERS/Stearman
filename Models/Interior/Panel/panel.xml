<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>panel.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>panel</object-name>

    <!-- Objets transparents -->
  </animation>

  <!-- compass -->
  <model>
    <path>Aircraft/Stearman/Models/Interior/Panel/Instruments/compass/compass.xml</path>
    <offsets>
      <x-m> 0.003 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 0.080 </z-m>
    </offsets>
  </model>

  <!-- alt -->
  <model>
    <path>Aircraft/Stearman/Models/Interior/Panel/Instruments/alt/alt.xml</path>
    <offsets>
      <x-m>  0.004 </x-m>
      <y-m> -0.110 </y-m>
      <z-m> -0.020 </z-m>
    </offsets>
  </model>

  <!-- asi -->
  <model>
    <path>Aircraft/Stearman/Models/Interior/Panel/Instruments/asi/asi.xml</path>
    <offsets>
      <x-m>  0.001 </x-m>
      <y-m> -0.040 </y-m>
      <z-m> -0.020 </z-m>
    </offsets>
  </model>

  <!-- rpm -->
  <model>
    <path>Aircraft/Stearman/Models/Interior/Panel/Instruments/rpm/rpm.xml</path>
    <offsets>
      <x-m>  0.005 </x-m>
      <y-m>  0.040 </y-m>
      <z-m> -0.020 </z-m>
    </offsets>
  </model>

  <!-- vsi -->
  <model>
    <path>Aircraft/Stearman/Models/Interior/Panel/Instruments/vsi/vsi.xml</path>
    <offsets>
      <x-m>  0.004 </x-m>
      <y-m> -0.080 </y-m>
      <z-m> -0.090 </z-m>
    </offsets>
  </model>

  <!-- turn -->
  <model>
    <path>Aircraft/Stearman/Models/Interior/Panel/Instruments/turn/turn.xml</path>
    <offsets>
      <x-m>  0.004 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.090 </z-m>
    </offsets>
  </model>

</PropertyList>

