<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>panel.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>panel</object-name>

    <!-- Objets transparents -->
  </animation>

  <!-- ALT -->
  <model>
    <path>Aircraft/Stearman/Models/Interior/Panel/Instruments/alt/alt.xml</path>
    <offsets>
      <x-m>  0.004 </x-m>
      <y-m> -0.110 </y-m>
      <z-m> -0.020 </z-m>
    </offsets>
  </model>
  
  <!-- ASI -->
  <model>
    <path>Aircraft/Stearman/Models/Interior/Panel/Instruments/asi/asi.xml</path>
    <offsets>
      <x-m>  0.001 </x-m>
      <y-m> -0.040 </y-m>
      <z-m> -0.020 </z-m>
    </offsets>
  </model>
  
  <!-- COMPASS -->
  <model>
    <path>Aircraft/Stearman/Models/Interior/Panel/Instruments/compass/compass.xml</path>
    <offsets>
      <x-m> 0.003 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 0.080 </z-m>
    </offsets>
  </model>
  
  <!-- MAGNETO -->
  <model>
    <path>Aircraft/Stearman/Models/Interior/Panel/Instruments/magneto/magneto.xml</path>
    <offsets>
      <x-m>  0.004 </x-m>
      <y-m> -0.180 </y-m>
      <z-m> -0.070 </z-m>
    </offsets>
  </model>

  <!-- MANIFOLD -->
  <model>
    <path>Aircraft/Stearman/Models/Interior/Panel/Instruments/man/man.xml</path>
    <offsets>
      <x-m>  0.004 </x-m>
      <y-m>  0.110 </y-m>
      <z-m> -0.020 </z-m>
    </offsets>
  </model>
  
  <!-- FUEL/OIL/TEMP -->
  <model>
    <path>Aircraft/Stearman/Models/Interior/Panel/Instruments/oil-fuel-temp/oil-fuel-temp.xml</path>
    <offsets>
      <x-m>  0.004 </x-m>
      <y-m>  0.080 </y-m>
      <z-m> -0.090 </z-m>
    </offsets>
  </model>
  
  <!-- RPM -->
  <model>
    <path>Aircraft/Stearman/Models/Interior/Panel/Instruments/rpm/rpm.xml</path>
    <offsets>
      <x-m>  0.005 </x-m>
      <y-m>  0.040 </y-m>
      <z-m> -0.020 </z-m>
    </offsets>
  </model>

  <!-- TURN -->
  <model>
    <path>Aircraft/Stearman/Models/Interior/Panel/Instruments/turn/turn.xml</path>
    <offsets>
      <x-m>  0.004 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.090 </z-m>
    </offsets>
  </model>
  
  <!-- VSI -->
  <model>
    <path>Aircraft/Stearman/Models/Interior/Panel/Instruments/vsi/vsi.xml</path>
    <offsets>
      <x-m>  0.004 </x-m>
      <y-m> -0.080 </y-m>
      <z-m> -0.090 </z-m>
    </offsets>
  </model>

</PropertyList>
